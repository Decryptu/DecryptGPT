    "repository": {
        "type": "git",
        "url": "git+https://github.com/Decryptu/DecryptGPT.git"
    },
    "bugs": {
        "url": "https://github.com/Decryptu/DecryptGPT/issues"
    },
    "homepage": "https://github.com/Decryptu/DecryptGPT#readme",
    "dependencies": {
        "date-fns": "^4.1.0",
        "discord.js": "^14.24.2",
        "dotenv": "^17.2.3",
        "openai": "^6.8.1"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.4",
        "@types/node": "^24.10.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3"
